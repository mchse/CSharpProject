@model List <Project.Models.Song>
@{
    ViewBag.Title = "Song Library";
    Layout = "~/Views/Shared/_Layout.cshtml";

    var cookie = Request.Cookies["CustID"];
    string userID = "";
    if (cookie != null)
    {
        userID = cookie.Value;
    }

    var cookieUsername = Request.Cookies["Username"];
    string Username = "";
    if (cookieUsername != null)
    {
        Username = cookieUsername.Value;
    }
}

<h2>Song Library</h2>

<table class="table songs table-striped">
    <thead>
        <tr>
            <th>Title</th>
            <th>Artist</th>
        </tr>
    </thead>
    <tbody>s
        @foreach (var song in Model)
        {
            <tr>
                <td>@song.Title</td>
                @*@<td>@Html.ActionLink(song.Title, "SongLibrary", "Song", new {id = song.Id}, null)</td>*@
                <td>@song.Artist</td>
                <td><a href="/Song/AddSongToCustomerLibrary/@song.Id">Add</a></td>
            </tr>
        }
    </tbody>
</table>
